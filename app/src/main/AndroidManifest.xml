<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pmp.tianxundai.com.notepaddemo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/log"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aty.IncidentAty" />
        <activity android:name=".aty.ScheduleAty" />
        <activity android:name=".aty.BackgroundAty" />
        <activity android:name=".aty.RingAty" />
        <activity android:name=".aty.MannerAty" />
        <activity android:name=".aty.DayAty" />
        <activity android:name=".aty.BasicAty" />
        <activity android:name=".aty.DayItemAty" />
        <activity android:name=".aty.RingActivity" />

        <receiver android:name=".guangbo.RingReceived">
            <intent-filter>
                <action android:name="com.example.cloakandnotifaction.shuting" />
            </intent-filter>
        </receiver>

        <activity android:name=".aty.NaolingActivity" />
        <activity android:name=".aty.Main2Activity" />
        <activity android:name=".aty.Main3Activity"></activity>
    </application>

</manifest>